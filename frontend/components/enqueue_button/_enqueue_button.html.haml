#enqueue_button
  
  -if @qt_diff[:diff] != 0
    -if @qt_diff[:qts]
      ="#{@qt_diff[:qts].count} tracks on queues"
    =form_tag enqueue_path, method: :post, remote: true do
      =submit_tag enqueue_button_text(@qt_diff[:diff], "scrub"), class: "btn btn-primary"
      =hidden_field_tag "context", @context
      =hidden_field_tag "parent_id", @parent_id
      =hidden_field_tag "queue", "scrub"
      -@qt_diff[:diff_tracks_ids].each do |id|
        =hidden_field_tag "ids[]", id
      
    
    -# =button_to enqueue_button_text(@qt_diff[:diff], "scrub"), enqueue_path, remote: true, class: "enqueue_button btn btn-primary", params: {context: @context, parent_id: @parent_id, ids: @qt_diff[:diff_tracks_ids].to_s}
  -else 
    .btn.btn-secondary
      All tracks queued
